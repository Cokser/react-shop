{
  "name": "react-shop",
  "version": "1.0.0",
  "description": "Webpack 3 React, Redux and Sass",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "clear && webpack-dev-server",
    "clean": "rimraf build",
    "lint": "eslint src || true && --quiet",
    "fix": "eslint src --fix && --quiet",
    "test": "clear && jest ./tests --setupTestFrameworkScriptFile=raf/polyfill",
    "run-dev": "webpack-dashboard -t 'Webpack Dashboard' -- webpack-dev-server",
    "build": "npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "deploy": "npm run build && cp now.json ./build && now ./build --public",
    "alias": "cp now.json ./build && cd ./build && now alias",
    "deploy-production": "npm run build && cp now.production.json ./build/now.json && now ./build",
    "alias-production": "cp now.production.json ./build/now.json && cd ./build && now alias"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Cokser/react-shop"
  },
  "keywords": [
    "react",
    "webpack3",
    "redux",
    "sass"
  ],
  "author": "Valeriy Khupenia",
  "bugs": {
    "url": "https://github.com/Cokser/react-shop/issues"
  },
  "homepage": "https://github.com/Cokser/react-shop/",
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "dotenv-webpack": "^1.5.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.8.3",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "^2.1.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.11.0",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.7",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "superagent": "^3.8.2"
  }
}
